{
  "manifest_version": 2,

  "name": "프로그래머스 코드실행 단축키",
  "description": "프로그래머스 실행 단축키입니다. ctrl+space로 실행하세요.",
  "version": "1.0.0",
  "icons":{
    "16" : "16.png",
    "24" : "24.png",
    "32" : "32.png"
  },
  "content_scripts": [
    {
        "matches": ["https://programmers.co.kr/*"],
        "js": ["content.js"]
    }
  ]
}